.PHONY = all clean help
EXE = wave

all:
	@cd src && $(MAKE) wave

clean:
	$(MAKE) clean
	@rm -f $(EXE) *~

help:
	@echo "Usage:"
	@echo "  make all             Build the software"
#	@echo "  make sudoku          Build the executable file from sudoku.c"
	@echo "  make clean           Remove all files generated by make"
